	<script type="text/javascript">

    // var demo, code, data, stars;

    // create instance
    window.onload = function() {
    	var aCount = 0;
    	var pCount = 0;
    	var eCount=  0;
		 <% if(typeof aCount != 'undefined' && aCount !=0) { console.log("aCount is" + aCount);%>
		 	aCount = <%=aCount%>;
		 <%}%>
		 <% if(typeof pCount != 'undefined' && pCount !=0) { console.log("pCount is" + pCount);%>
		 	pCount = <%=pCount%>;
		 <%}%>
		 <% if(typeof eCount != 'undefined' && eCount !=0) { console.log("eCount is" + eCount);%>
		 	eCount = <%=eCount%>;
		 <%}%>

        // setup countUp object 
        if(aCount != 0) {
	        artistsCount = new countUp('artistsCount', 0, aCount);
	        artistsCount.start(methodToCallOnComplete);
        }
        
        
        if(pCount != 0) {
	        postsCount = new countUp('postsCount', 0, pCount);
	        postsCount.start();
	    }    

	    if(eCount != 0) {
	        eventCount = new countUp('eventCount', 0, eCount);
	        eventCount.start();
	    }    
    }


    function methodToCallOnComplete() {
    	$('.tlt').textillate({ in: { effect: 'flipInY'}});
    	$('.tlt').find('.texts li:first').text("The cat is out of the bag!");
        $('.tlt').textillate('start');

    }



    </script>


      <div class="panel-group notification-panel">
        <% if (typeof users != 'undefined') { 
                  for(var i=0; i < users.length; i++) {
                    var userRole = "";
                    var userCity = "";
                    if(typeof users[i] != 'undefined') {
                        if(typeof users[i].local.role == 'undefined' || users[i].local.role.length == 0) {
                              userRole = "Not updated yet"
                        } else {
                              userRole = users[i].local.role
                        }           

                        if(typeof users[i].local.city == 'undefined' || users[i].local.city == "") {
                              userCity = "Not Set"
                        } else { 
                              userCity = users[i].local.city
                        } 
                    }%>
                    <div style="padding-left:25px;padding-right:25px;">
                      <a href = "/profile?fbId=<%=users[i].facebook.id%>" style="max-width:150px;max-height:150px;">
                        <img src="<%= users[i].local.picture %>" border=0   style="max-width:150px;max-height:150px;"/>
                        <span style="color:#FFF9F9;font-size:small;position:relative;width:100%;"><%=users[i].facebook.name%></span>
                        <span style="color:#FFF9F9;font-size:small;position:relative;width:100%;"> from <%= userCity %></span>

                      </a>
                     </div> 
                     <div>&nbsp;</div>
                  <%}%>     
          <%}%>

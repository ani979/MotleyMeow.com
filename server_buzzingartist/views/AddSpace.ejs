		<h1>Add Space</h1>
		<h6> Fill the form to add any performance/rehearsal space.</h6>
		<div class = "container">
			<div class="login">

				<form id="signupp" action="/AddSpace" method="post">
					<% if (message.length > 0) { %>
	        		<div class="alert alert-danger" style="font-size:20px;"><%= message %></div>
	    			<% } %>
					<ul>
						<li>
							<a href="#" class=" icon user"></a><select type="text" class="text" id="typeofspace" name="typeofspace"  placeholder="Type of Space"  required tabindex=1>
							 <option value="Performance Space">Performance Space</option>
 							 <option value="Rehearsal space">Rehearsal space</option>
 							</select>
						</li>
						<li>
							<a href="#" class=" icon mail"></a><input type="text" class="text" id="nameofspace" name="nameofspace" placeholder="Name of Space"  required tabindex=2>
						</li>
						<div class="dumy">
						<li class="psw">
							<a href="#" class=" icon key"></a><select type="text" id="city" name="city" placeholder="City" required tabindex=3>
							<% var cities = dropdowns.alldropdowns.cities;
                  for(var i = 0; i < cities.length; i ++) {   %> 
                        <option value = "<%=cities[i].city%>"><%=cities[i].text%></option>
            <!--  <option>Bengaluru</option><option>Chennai</option><option>Delhi</option><option>Mumbai</option><option>Kolkata</option><option>None Of the above</option></select>--> 
              <% } %>
          				</select>
						</li>
						<li class="psw two">
							<a href="#" class=" icon key"></a><select type="text" id="location" name="location" placeholder="location" required tabindex=4>
								<% var Bangluruloc = dropdowns.alldropdowns.Bangluruloc;
                  for(var i = 0; i < Bangluruloc.length; i ++) {   %> 
                        <option value = "<%=Bangluruloc[i].location%>"><%=Bangluruloc[i].text%></option>
            <!--  <option>Bengaluru</option><option>Chennai</option><option>Delhi</option><option>Mumbai</option><option>Kolkata</option><option>None Of the above</option></select>--> 
              <% } %>		 
							 </select>
						</li>
						<li class="psw">
							<a href="#" class=" icon key"></a><input type="number" id="charge" name="charge" placeholder="Charge per day"  tabindex=5></li>
						<li class="psw two">
							<a href="#" class=" icon key"></a><input type="number" id="capacity" name="capacity" placeholder="Sitting Capacity" required tabindex=6>
						</li>
            <li>
              <a href="#" class=" icon mail"></a><input type="text" class="text" id="contactperson" name="contactperson" placeholder="Name of contact Person"  required tabindex=7>
            </li>
             <li>
              <a href="#" class=" icon mail"></a><input type="number" class="text" id="contactno" name="contactno" placeholder="Contact No"  required tabindex=8>
            </li>


						<div class="clear"></div>
						<div class="submit" style="margin-top:30px;">
								<input type="submit" value="SUBMIT SPACE" >
								
				       	</div>
				       		
				     </ul>
				</form>
				
		   </div>
